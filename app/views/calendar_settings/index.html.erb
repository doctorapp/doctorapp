<h1>Listing calendar_settings</h1>

<table>
  <tr>
    <th>Doctor</th>
    <th>Slotminutes</th>
    <th>Mintime</th>
    <th>Maxtime</th>
    <th>Firsthour</th>
    <th>Defaulteventminutes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @calendar_settings.each do |calendar_setting| %>
  <tr>
    <td><%= calendar_setting.doctor_id %></td>
    <td><%= calendar_setting.slotMinutes %></td>
    <td><%= calendar_setting.minTime %></td>
    <td><%= calendar_setting.maxTime %></td>
    <td><%= calendar_setting.firstHour %></td>
    <td><%= calendar_setting.defaultEventMinutes %></td>
    <td><%= link_to 'Show', calendar_setting %></td>
    <td><%= link_to 'Edit', edit_calendar_setting_path(calendar_setting) %></td>
    <td><%= link_to 'Destroy', calendar_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

