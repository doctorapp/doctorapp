  <div class="field">
    <%= f.label :minTime %><br />
    <%= f.number_field :minTime, defalut: 8 %>
  </div>
  <div class="field">
    <%= f.label :maxTime %><br />
    <%= f.number_field :maxTime, default: 18%>
  </div>
  <div class="field">
    <%= f.label :firstHour %><br />
    <%= f.number_field :firstHour, default: 8 %>
  </div>
  <div class="field">
    <%= f.label :defaultEventMinutes %><br />
    <%= f.number_field :defaultEventMinutes, default: 60 %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
