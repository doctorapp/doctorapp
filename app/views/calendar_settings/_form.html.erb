<%= form_for(@calendar_setting) do |f| %>
  <% if @calendar_setting.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@calendar_setting.errors.count, "error") %> prohibited this calendar_setting from being saved:</h2>

      <ul>
      <% @calendar_setting.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  
  <div class="field">
    <%= f.label :slotMinutes %><br />
    <%= f.number_field :slotMinutes, default: 30, :step => 5 %>
